{
    "sites": [
        {
            "connections": [
                {
                    "engine": "django.db.backends.mysql",
                    "name": "test_database",
                    "port": "3306",
                    "host": "127.0.0.1",
                    "user": "root",
                    "password": "",
                    "id": "test_connection"
                }
            ],
            "user_pk": "users.id@test_connection",
            "name": "test_connection",
            "properties": [
                {
                    "source": "^Person.first_name(@gender,Male,Female)",
                    "type": "VARCHAR(255)",
                    "name": "firstname"
                },
                {
                    "source": "^Person.last_name_initial()",
                    "type": "VARCHAR(255)",
                    "name": "lastname"
                },
                {
                    "source": "users.gender@test_connection",
                    "type": "text",
                    "name": "gender"
                },
                {
                    "source": "users.age@test_connection",
                    "type": "mediumint(9)",
                    "name": "age"
                },
                {
                    "source": "users.address@test_connection",
                    "type": "varchar(255)",
                    "name": "address"
                },
                {
                    "aggregate": "avg",
                    "source": "running.duration@test_connection",
                    "user_fk": "running.user@test_connection",
                    "type": "FLOAT",
                    "name": "running_duration"
                }
            ]
        }
    ]
}